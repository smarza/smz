{
  "name": "@smz-ui/core",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/smz-core/src",
  "prefix": "lib",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": ["dist/{projectRoot}"],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/smz-core/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/smz-core/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/smz-core/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
    ,
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "main": "libs/smz-core/src/test.ts",
        "tsConfig": "libs/smz-core/tsconfig.spec.json",
        "karmaConfig": "libs/smz-core/karma.conf.js"
      }
    }
  }
}
